#pragma once

/* control use of restrict keyword: not defined for C++ and C90 */
#if defined(__cplusplus) || (defined(__ASTREE_CLANG_COMPATIBLE) && !(defined(__STDC_VERSION__) && __STDC_VERSION__ >= 199901L))
#define __ASTREE_restrict
#else
#define __ASTREE_restrict restrict
#endif

/* RULECHECKER_suppress(rules-category) */
__ASTREE_attributes((coverage_ignore, raise_at_caller, hide_directives));
